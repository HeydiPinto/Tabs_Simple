<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.heydi.tabs_simple">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/MyMaterialTheme">
        <activity android:name=".activity.Incio">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".activity.Main2Activity" android:parentActivityName=".activity.Incio" />
        <activity android:name=".activity.Main5Activity" android:parentActivityName=".activity.Incio"/>
        <activity android:name=".activity.MainActivity" android:parentActivityName=".activity.Incio">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" android:parentActivityName=".activity.Incio"/>

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".activity.Main3Activity" android:parentActivityName=".activity.Incio"/>
        <activity android:name=".activity.Main4Activity" android:parentActivityName=".activity.Incio"></activity>
    </application>

</manifest>